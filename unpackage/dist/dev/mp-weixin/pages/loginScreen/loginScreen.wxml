<view class="login-page"><view class="login-card"><view class="login-item flex-row border-bottom"><image class="label" style="width:52rpx;height:52rpx;" src="/static/images/phone.png"></image><input class="value flex-1" type="number" maxlength="11" placeholder="请输入您的账号" placeholder-style="color: #D1D1D1" data-event-opts="{{[['input',[['__set_model',['$0','mobile','$event',[]],['formData']]]]]}}" value="{{formData.mobile}}" bindinput="__e"/></view><view class="login-item flex-row border-bottom"><image class="label" style="width:52rpx;height:52rpx;" src="/static/images/password.png"></image><input class="value flex-1" type="text" placeholder="请输入您的密码" placeholder-style="color: #D1D1D1" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['formData']]]]]}}" value="{{formData.password}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['login',['$event']]]]]}}" class="submit-btn flex-col" bindtap="__e"><text>登录</text></view></view></view>